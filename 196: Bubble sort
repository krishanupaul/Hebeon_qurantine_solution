#include<iostream>
  using namespace std;
  int main()
  {
      int n;
      int i, j,temp;
      cin>>n;
      int arr[n];
      
      for(i=0;i<n;i++){
          cin>>arr[i];
      }
        
        for (i=0;i<n-1;i++){     
            for (j=0;j<n-i-1;j++){  
                if (arr[j] > arr[j+1]){
                    temp = arr[j];  
                    arr[j] = arr[j+1];  
                    arr[j+1] = temp;
                }
            }
        }
    cout<<"Enter the size of array: Enter the array elements: Array after bubble sort:";
    for(i=0;i<n;i++){
        cout<<" "<<arr[i];
    }
    return 0;
  }
